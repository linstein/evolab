{
  "name": "@evolab/filebrowser-extension",
  "version": "2.2.0",
  "description": "JupyterLab - Filebrowser Widget Extension",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "schema/*.json",
    "style/**/*.css"
  ],
  "sideEffects": [
    "style/**/*.css"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@evolab/application": "^2.2.0",
    "@evolab/apputils": "^2.2.0",
    "@evolab/coreutils": "^4.2.0",
    "@evolab/docmanager": "^2.2.0",
    "@evolab/filebrowser": "^2.2.0",
    "@evolab/launcher": "^2.2.0",
    "@evolab/mainmenu": "^2.2.0",
    "@evolab/services": "^5.2.0",
    "@evolab/settingregistry": "^2.2.0",
    "@evolab/statedb": "^2.2.0",
    "@evolab/statusbar": "^2.2.0",
    "@evolab/ui-components": "^2.2.0",
    "@lumino/algorithm": "^1.2.3",
    "@lumino/commands": "^1.10.1",
    "@lumino/messaging": "^1.3.3",
    "@lumino/widgets": "^1.11.1"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typedoc": "^0.15.4",
    "typescript": "~3.7.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
